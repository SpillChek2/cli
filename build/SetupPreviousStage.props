<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
    <PropertyGroup Condition="'$(PreviousStageProps)' == ''">
      <PreviousStageDirectory>$([MSBuild]::NormalizePath($(RepoRoot)/.dotnet_stage0/$(Architecture)))</PreviousStageDirectory>
      <PreviousStageDotnet>$([MSBuild]::NormalizePath($(PreviousStageDirectory)/dotnet$(ExeExtension)))</PreviousStageDotnet>
    </PropertyGroup>

    <Import Project="$(PreviousStageProps)" Condition="'$(PreviousStageProps)' != ''" />

    <PropertyGroup>
      <IsDesktopAvailable>False</IsDesktopAvailable>
      <IsDesktopAvailable Condition=" '$(OS)' == 'Windows_NT' ">True</IsDesktopAvailable>
    </PropertyGroup>
</Project>
